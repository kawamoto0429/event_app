<div class="h1 text-center">サークル制作</div>
  <%= form_for(@circle) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>
    <div class="mt-2">
      <label class="form-label">サークル名</label>
      <%= f.text_field :name, class: 'form-control' %>
    </div>
    <div class="field">
      <label class="form-label">サークル内容</label>
      <%= f.text_area :body, class: 'form-control' %>
    </div>
    <div class="mx-auto" style="width: 10rem;">
    <%= f.submit "作成", class: "btn btn-primary btn-block" %>
  </div>
  <% end %>
</div>